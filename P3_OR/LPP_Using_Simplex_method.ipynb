      "source": [
        "from scipy.optimize import linprog\r\n",
        "\r\n",
        "obj=[-200,300]\r\n",
        "\r\n",
        "lhs_ineq=[[-2,-3],[1,1],[-2,-1.5]]\r\n",
        "\r\n",
        "rhs_ineq=[-1200,400,-900]\r\n",
        "\r\n",
        "bnd=[(0,float(\"inf\")),(0,float(\"inf\"))]\r\n",
        "\r\n",
        "opt=linprog(c=obj,A_ub=lhs_ineq, b_ub=rhs_ineq,bounds=bnd,method=\"revised simplex\")\r\n",
        "\r\n",
        "opt\r\n"
      ],
